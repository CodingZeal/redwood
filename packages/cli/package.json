{
  "name": "@redwoodjs/cli",
  "version": "3.2.0",
  "description": "The Redwood Command Line",
  "repository": {
    "type": "git",
    "url": "https://github.com/redwoodjs/redwood.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "bin": {
    "redwood": "./dist/index.js",
    "rw": "./dist/index.js",
    "rwfw": "./dist/rwfw.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build:js",
    "clean": "rimraf dist",
    "build:clean": "yarn clean && yarn build",
    "clean:tests": "rimraf 'dist/**/*/__tests__'",
    "babel": "babel src -d dist --extensions \".js,.ts,.tsx\" --copy-files --no-copy-ignored",
    "build:js": "yarn babel && yarn clean:tests",
    "build:debug": "yarn clean && yarn babel --source-maps inline --retain-lines",
    "build:watch": "yarn nodemon \"yarn build:clean && yarn fix:permissions\"",
    "build:watch:debug": "yarn nodemon 'yarn build:debug'",
    "nodemon": "nodemon --watch src --ext \"js,ts,tsx,template\" --ignore dist --exec",
    "dev": "node dist/index.js",
    "dev:example": "RWJS_CWD=../../__fixtures__/example-todo-main node dist/index.js",
    "fix:permissions": "chmod +x dist/index.js dist/rwfw.js",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.20.7",
    "@prisma/internals": "4.8.0",
    "@redwoodjs/api-server": "3.2.0",
    "@redwoodjs/cli-helpers": "3.2.0",
    "@redwoodjs/internal": "3.2.0",
    "@redwoodjs/prerender": "3.2.0",
    "@redwoodjs/structure": "3.2.0",
    "@redwoodjs/telemetry": "3.2.0",
    "@types/secure-random-password": "0.2.1",
    "@vercel/nft": "^0.22.6",
    "boxen": "5.1.2",
    "camelcase": "6.3.0",
    "chalk": "4.1.2",
    "concurrently": "7.6.0",
    "configstore": "3.1.5",
    "core-js": "3.26.1",
    "cross-env": "7.0.3",
    "crypto-js": "4.1.1",
    "decamelize": "5.0.0",
    "dotenv-defaults": "5.0.2",
    "enquirer": "2.3.6",
    "envinfo": "7.8.1",
    "execa": "5.1.1",
    "fast-glob": "3.2.12",
    "findup-sync": "5.0.0",
    "fs-extra": "11.1.0",
    "latest-version": "5.1.0",
    "listr2": "5.0.6",
    "lodash": "4.17.21",
    "param-case": "3.0.4",
    "pascalcase": "1.0.0",
    "pluralize": "8.0.0",
    "portfinder": "1.0.32",
    "prettier": "2.8.1",
    "prisma": "4.8.0",
    "prompts": "2.4.2",
    "rimraf": "3.0.2",
    "secure-random-password": "0.2.3",
    "terminal-link": "2.1.1",
    "title-case": "3.0.3",
    "yargs": "17.6.2"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.7",
    "@types/crypto-js": "4.1.1",
    "jest": "29.3.1",
    "nodemon": "2.0.20",
    "typescript": "4.7.4"
  },
  "volta": {
    "node": "16.19.0",
    "yarn": "3.1.1"
  }
}
